programa {
  funcao inicio() {
    real num
    escreva("Algoritmo p/ testar se um número é maior que 10\n")
    escreva("Digite um número: ")
    leia(num)
    se (num>10)
    {
      escreva("O número digitado é maior que 10.\n")
    }
    senao {
      escreva("O número é menor ou igual a 10.\n")
    }
    escreva("Fim do algoritmo.")
  }
}

